
.tap {
  width: 750rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: rgba(255, 255, 255, 1);
  display: flex;
  justify-content: space-around;
}

.tap-tex {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.tap-this {
  color: #773adf;
}

.tap-this::after {
  content: "";
  display: block;
  width: 40rpx;
  height: 8rpx;
  background-color: #773adf;
  border-radius: 10rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  margin-top: -15rpx;
}
.none-img {
  width: 360rpx;
  height: 360rpx;
  margin-left: 195rpx;
}
.return-btn {
  width: 200rpx;
  height: 78rpx;
  line-height: 78rpx;
  border: 1px solid rgba(102, 102, 102, 1);
  border-radius: 100px;
  font-size: 30rpx;
  font-weight: 400;
  color: #666;
  text-align: center;
  margin: 145rpx auto 0 auto;
}

.cupon-item-container {
  margin-bottom: 150rpx;
}

.coupon-item {
  position: relative;
  height: 240 rpx;
  margin: 20rpx 20rpx 0 20rpx;
  border-radius: 10rpx;
}

.coupon-view {
  display: flex;
  justify-content:space-around;
  background: #fff;
  height: 210rpx;
  width: 520rpx;
  padding: 40rpx 0 40rpx 30rpx;
  box-sizing: border-box;
}

.get-coupon-action {
  position: fixed;
  width: 100%;
  background: #f5f5f5;
  bottom: 0rpx;
  z-index: 100;
}
.lfBt{
  font-size: 26rpx;
  color: #FC4C4C;
}
.lfTop{
  display: flex;
  font-size: 50rpx;
  color: #FC4C4C;
  align-items: center
}
.lfTop view{
  font-weight: 700rpx;
}
.lfTop text{
  font-size: 32rpx;
  margin-top:15rpx;
}
.rt{
  width: 286rpx;
}
.rtTop{
  font-size: 26rpx;
  font-weight: 500;
  color: #000;
  margin-bottom: 10rpx;
}
.rtBt{
  font-size: 24rpx;
  color: #999;
  width: 100%;
  display: flex
}
.get-coupon-btn {
  width: 630rpx;
  margin: 20rpx auto;
  line-height: 80rpx;
  height: 80rpx;
  border: none;
  border-radius: 60rpx;
}
.use-btn {
  background: #fff;
  position: absolute;
  width: 200rpx;
  height: 210rpx;
  right: 0;
  top: 0;
}
.positionImg{
  height: 76rpx;
  width: 76rpx;
  top:1rpx;
  left:0;
  position: absolute;
}
.use-btn-bn {
  border-radius:40rpx;
  border: none;
  height: 60rpx;
  line-height: 60rpx;
  width: 140rpx;
  font-size:28rpx;
  position: absolute;
  top: 80rpx;
  right: 25rpx;
  background: linear-gradient(to right,#AC3EFF, #7739DF);
  color:#fff;
}
.coupon-type {
  position: absolute;
  top: 10rpx;
  left: 0rpx;
  font-size: 12px;
  height: 48rpx;
  line-height: 48rpx;
  padding-left: 20rpx;
  width: 220rpx;
  color: #fff;
}
.coupon-type:after {
  height: 0rpx;
  width: 0rpx;
  content: '';
  border: 25rpx solid transparent;
  border-right: 25rpx solid #fff;
  position: absolute;
  right: -0rpx;
}
.coupon-type1 {
  background: red;
}
.coupon-type2 {
  background: red;
}
.coupon-type3 {
  background: #773adf !important;
}
.name {
  position: absolute;
  top: 10rpx;
  line-height: 48rpx;
  left: 260rpx;
  color: #666;
  font-size: 12px;
}
.discount {
  position: absolute;
  top: 60rpx;
  left: 20rpx;
  color: #333;
  font-size: 18px;
}
.minimum {
  position: absolute;
  top: 120rpx;
  color: #999;
  left: 20rpx;
  font-size: 14px;
}
.time {
  position: absolute;
  top: 175rpx;
  left: 20rpx;
  font-size: 12px;
  color: #ccc;
}
.coupon-used-view, .coupon-disabled-view {
  position: absolute;
  top: 40rpx;
  right: 140rpx;
}
.coupon-used-view .image, .coupon-disabled-view .image {
  width: 160rpx;
  height: 160rpx;
}
.cupon-item-container-disabled .coupon-type {
  background: #ddd !important;
}
.cupon-item-container-disabled .use-btn-bn {
  background: #ddd !important;
  color: #aaa !important;
}
 
.cupon-item-container-disabled .rtTop{ 
  color: #ddd !important;
}
.cupon-item-container-disabled .lfTop{ 
  color: #ddd !important;
}
.cupon-item-container-disabled .rtBt{ 
  color: #ddd !important;
}
.cupon-item-container-disabled  .lfBt{ 
  color: #ddd !important;
}
