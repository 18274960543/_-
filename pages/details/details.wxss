/* 黑色弹框 */

.shadow {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.8;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 101;
}

.spec_shadow {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  padding: 30rpx 40rpx;
  box-sizing: border-box;
  background: #fff;
  z-index: 102;
  /* height: 60%; */
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

