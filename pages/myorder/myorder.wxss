.top {
  background: #fff;
  padding-top: 15rpx;
}

.top_list {
  display: flex;
  width: 540rpx;
  border: 1rpx solid #eee;
  border-radius: 10rpx;
  margin: 0 auto;
  height: 50rpx;
}

.top_list view {
  flex: 1;
  font-size: 28rpx;
  color: #333;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}

.top_list .active {
  background: #ffc408;
  color: #fff;
  border-radius: 5rpx;
  line-height: 58rpx;
  height: 58rpx;
  margin-top: -5rpx;
}

.top_bt {
  width: 100%;
  height: 70rpx;
  white-space: nowrap;
  line-height: 70rpx;
  border-top: 1rpx solid #eee;
  margin-top: 15rpx;
}

.top_bt view {
  display: inline-block;
  font-size: 24rpx;
  color: #333;
  text-align: center;
  padding: 0 38rpx;
  position: relative;
}

.top_bt view.top_bt_active {
  color: #ffc408;
}

.top_bt_active::after {
  content: "";
  display: block;
  position: absolute;
  height: 5rpx;
  width: 30rpx;
  background: #773adf;
  left: 36%;
  bottom: 10rpx;
  border-radius: 5rpx;
}

/* 订单列表 */

.house {
  margin-bottom: 25rpx;
  background: #fff;
  border-radius: 10rpx;
  margin: 30rpx;
}

.house-top {
  display: flex;
  padding: 35rpx 30rpx;
  justify-content: space-between;
  align-items: center;
}

.house-top>view {
  display: flex;
  align-items: center;
}

.house-top text {
  max-width: 400rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: 600;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.house-top image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 3rpx;
}

.house-content-left {
  height: 120rpx;
  width: 120rpx;
  background: #fff;
  margin-right: 20rpx;
}

.house-content {
  display: flex;
  background: #fafafa;
  padding: 25rpx 30rpx 15rpx;
}

.house-content-left image {
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx auto;
  display: block;
}

.house-content-right {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.house-content-top {
  font-size: 24rpx;
  color: #333;
  font-weight: 600;
  height: 65rpx;
  overflow: hidden;
  margin-bottom: 0rpx;
}

.house-content-bt {
  display: flex;
  flex-direction: column;
}

.house-content-bt view {
  font-size: 24rpx;
  color: #fc4c4c;
}

.house-content-bt text {
  font-size: 24rpx;
  color: #666;
}

.house-bt {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0;
  align-items: center;
  margin: 0 20rpx;
  font-size: 28rpx;
  color: #333;
  border-bottom: 1px solid #f5f5f5;
}

.house-bt-right {
  display: flex;
  align-items: center;
}

.house-bt-right image {
  width: 14rpx;
  height: 25rpx;
  margin-left: 20rpx;
}

.house-bt-right textarea {
  height: 40rpx;
  width: 370rpx;
}

.house-top_rt {
  font-size: 26rpx;
  color: #999;
}

.payment {
  display: flex;
  justify-content: flex-end;
  padding: 10rpx 0;
}

.house-bt-left {
  margin-right: 20rpx;
}

.payment view {
  font-size: 23rpx;
  color: #707070;
  padding: 10rpx 20rpx;
  border: 1px solid #666;
  border-radius: 50rpx;
  margin-right: 20rpx;
  margin-left: 10px;
}

.img {
  display: flex;
}

.img image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 10rpx;
}

.bt {
  padding: 30rpx 25rpx;
}

.rt {
  font-size: 24rpx;
  color: #333;
}

.bt_top {
  display: flex;
  font-size: 28rpx;
  color: #333;
  justify-content: space-between;
  border-bottom: 1rpx solid #eee;
  padding-bottom: 25rpx;
}

.bt_top_rt {
  display: flex;
}

.bt_top_rt view {
  display: flex;
  align-items: center;
}

.bt_top_rt view image {
  height: 28rpx;
  width: 28rpx;
  margin-right: 10rpx;
}

.bt_top_rt text {
  height: 25rpx;
  width: 2rpx;
  background: #f3f3f3;
  margin: 8rpx 20rpx 0;
}

.bt_center {
  display: flex;
  justify-content: space-between;
  padding: 35rpx 0;
}

.bt_center view {
  width: 430rpx;
  display: flex;
  flex-direction: column;
}

.bt_center view view {
  font-size: 28rpx;
  color: #333;
  margin-bottom: 35rpx;
}

.bt_center view text {
  font-size: 24rpx;
  color: #666;
}

.bt_center image {
  height: 147rpx;
  width: 147rpx;
}

.bt_bt {
  display: flex;
  justify-content: flex-end;
}

.bt_bt view {
  padding: 10rpx 20rpx;
  border-radius: 100rpx;
  border: 2rpx solid #666;
  font-size: 23rpx;
  color: #707070;
  margin-left: 25rpx;
}

