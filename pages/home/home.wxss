/* 地理位置定位 */

.position {
  /* position: absolute;
  top: 0;
  left: 0; */
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
  font-size: 32rpx;
  color: #333;
  background: #fff;
  width: 90%;
  height: 30rpx;
  padding: 5rpx 5% 80rpx;
  border-radius: 0 0 50px 50px;
  justify-content: flex-start;
  z-index: 111;
}

.position image {
  height: 22rpx;
  width: 25rpx;
}

.position .position-dw {
  height: 29rpx;
  width: 24rpx;
}

.position text {
  width:99%;
  padding-left: 6rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}

.position view {
  float: right;
  margin-left: 50%;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color: #ffc408;
}

.position view image {
  height: 40rpx;
  width: 40rpx;
}

/*阴影弹框*/

.mask {
  background: #000;
  opacity: 0.5;
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  top: 0;
  width: 100%;
  z-index: 201;
}

.none {
  display: none;
}

.coupon {
  position: fixed;
  width: 560rpx;
  left: 82rpx;
  height: 700rpx;
  top: 10%;
  z-index: 202;
}

.coupon image {
  height: 660rpx;
  display: flex;
  width: 100%;
}

.coupon image.icon {
  height: 60rpx;
  width: 60rpx;
  margin: 0 auto;
  margin-top: 80rpx;
}

.ot_price_rmb {
  margin-left: 10rpx;
  text-decoration: line-through;
  font-size: 20rpx;
  color: #666;
}

.indexImages {
  height: 300rpx;
  width: 500rpx;
  margin: 0 auto;
  padding-top: 300rpx;
}

.indexImages image {
  height: 500rpx !important;
  width: 100%;
  display: block;
}

.indexImages view {
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 50rpx auto;
  text-align: center;
  border-radius: 10rpx;
  font-size: 26rpx;
}

/* 限量抢购  */

.limited {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.limited-lf {
  width: 49%;
  background: #fff;
  height: 390rpx;
  border-top-left-radius: 10rpx;
}

.limited-rt {
  width: 49%;
}

.limited-time {
  font-size: 28rpx;
  display: flex;
  padding-top: 18rpx;
  margin: 0 0 50rpx 7%;
}

.limited-time-1 {
  font-size: 20rpx;
  display: flex;
  margin-left: 18rpx;
  margin-top: -1rpx;
}

.limited-time-1 view:nth-child(odd) {
  background: #000;
  color: #fff;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 100rpx;
  text-align: center;
}

.limited-time-1 view:nth-child(even) {
  margin: 0 10rpx;
}

.limited-lf-goods {
  width: 90%;
  margin: 0 auto;
  display: flex;
  font-size: 24rpx;
}

.limited-lf-goodsList {
  width: 50%;
  text-align: center;
}

.limited-lf-goods-img image {
  width: 155rpx;
  height: 155rpx;
}

.limited-lf-goods-info {
  text-align: center;
}

.limited-lf-goods-info view:nth-child(1) {
  color: #fc4c4c;
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 12rpx;
}

.limited-lf-goods-info view:nth-child(2) {
  color: #999;
}

.limited-lf-goods-info view:nth-child(2) text {
  color: #333;
}

.limited-top {
  width: 100%;
  background: #fff;
  height: 190rpx;
  border-top-right-radius: 10rpx;
}

.limited-bot {
  width: 100%;
  background: #fff;
  height: 190rpx;
  margin-top: 10rpx;
}

.limited-bot view:nth-child(1) text {
  font-size: 24rpx;
  color: #999;
}

.limited-top view:nth-child(1), .limited-bot view:nth-child(1) {
  font-size: 28rpx;
  font-weight: bold;
  padding-top: 16rpx;
  margin-left: 9%;
  margin-bottom: 10rpx;
}

.limited-top view:nth-child(2) image, .limited-bot view:nth-child(2) image {
  width: 116rpx;
  height: 124rpx;
  margin: 0 6%;
}

