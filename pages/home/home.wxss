/* 热卖专区 */

.popularAreas {
  width: 100%;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}

.areasTop {
  display: flex;
  height: 100rpx;
  align-items: center;
  padding: 23rpx 20rpx;
  box-sizing: border-box;
  justify-content: space-between;
  font-size: 32rpx;
  color: #fff;
  background-size: 100% 100%;
  background-position:let top;
}

.areasTop image {
  height: 55rpx;
  width: 55rpx;
}

.areasTop view {
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 50rpx;
  background: linear-gradient(#ffe6a1, #fbd15f);
  text-align: center;
  color: #ff5038;
  font-size: 26rpx;
}

.areasTop text {
  margin-left: 60rpx;
  padding: 0 20rpx;
}

.scrollLeft {
  width: 100%;
  white-space: nowrap;
  display: flex;
  align-items: center;
}

.slide {
  display: inline-block;
  width: 260rpx;
  margin-left: 25rpx;
  padding: 15rpx 0;
}

.slide>image {
  height: 160rpx;
  width: 125rpx;
  display: block;
  margin: 20rpx auto;
}

.slideText {
  width: 100%;
  font-size: 26rpx;
  color: #343434;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.lfRt {
  font-size: 20rpx;
  color: #666;
  display: flex;
  align-items: center
}

.lfRt text {
  padding-right: 30rpx;
}

.slideBt {
  align-items: center;
  display: flex;
  padding: 15rpx;
}

.slideBt image {
  height: 30rpx;
  width: 30rpx;
}

.slideBt view {
  font-size: 24rpx;
  color: #fc4c4c;
  margin-right: 13rpx;
}

.slideBt view text {
  font-size: 20rpx;
}

.slideBt>text {
  font-size: 18rpx;
  color: #999;
  margin-right: 30rpx;
  display: flex;
  align-items: center;
  flex: 1;
}

