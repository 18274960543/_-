/* 物流 */

.showCourier {
width: 100%;
/* height: 600rpx; */
position: fixed;
bottom: 0;
z-index: 9;
background: #fff;
}

.showCourier-status {
width: 90%;
margin: 0 auto;
height: 100rpx;
display: flex;
justify-content: space-between;
}

.showCourier-status view {
line-height: 100rpx;
}

.showCourier-1 {
width: 100%;
height: 100rpx;
font-size: 30rpx;
}

.showCourier-1 input {
width: 95%;
height: 100%;
padding-left: 5%;
border-bottom: 1px solid #eee;
}

.showCourier-2 {
width: 100%;
height: 400rpx;
margin-top: 20rpx;
}

.showCourier-2 scroll-view {
width: 100%;
height: 100%;
}

.scrollItem {
width: 90%;
margin: 0 auto;
position: relative;
height: 80rpx;
}

.scrollItem view:nth-child(1) {
font-size: 28rpx;
line-height: 80rpx;
}

.scrollItem-quan {
border: 1px solid #333;
width: 28rpx;
height: 28rpx;
border-radius: 50%;
position: absolute;
top: 36%;
right: 5%;
display: flex;
align-items: center;
justify-content: center;
}

.scrollItem-quan view {
background: #333;
width: 20rpx;
height: 20rpx;
border-radius: 50%;
position: absolute;
top: 5rpx;
left: 5rpx;
}

/* 联系客服 */

.ServicePopups {
position: fixed;
bottom: 0%;
width: 100%;
min-height: 500rpx;
background: #fff;
font-size: 28rpx;
padding-bottom: 38rpx;
z-index:9;

}

.ServicePopups-1 {
width: 100%;
height: 100rpx;
line-height: 100rpx;
text-align: center;
font-weight: bold;
font-size: 32rpx;
}

.ServicePopups-2 {
width: 95%;
position: relative;
padding-left: 5%;
margin-top: 38rpx;
}

/* .ServicePopups-2 view:nth-child(2) {
font-size: 24rpx;
} */

.ServicePopups-2 view:nth-child(3) {
width: 120rpx;
height: 25rpx;
line-height: 25rpx;
text-align: center;
position: absolute;
right: 6%;
top: 0%;
border-left: 1px solid #999;
color: #999;
}

.ServicePopups-3 {
width: 95%;
padding-left: 5%;
font-weight: bold;
}

.ServicePopups-close {
width: 64rpx;
height: 64rpx;
position: absolute;
top: 3%;
right: 5%;
}
.mask{
  position: fixed;
  top:0;
  left: 0;
  bottom: 0;
  right: 0;
  background: #000;
  opacity: 0.5;
  z-index: 8
}

