/* 弹框显示动画 */
.screeningShow {
  animation: 0.4s showscre;
}
.screeningHide {
  animation: 0.4s hidescre;
}
@keyframes showscre {
  0% {
    transform: translateY(500rpx);
  }

  100% {
    transform: translateY(0);
  }
}
@keyframes hidescre {
  0% {
    transform: translateY(0);
  }

  100% {
    transform: translateY(500rpx);
  }
}
